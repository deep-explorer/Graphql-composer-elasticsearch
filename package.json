{
  "name": "graphql-compose-elasticsearch",
  "version": "0.0.0-semantically-released",
  "description": "Elastic search via GraphQL",
  "files": [
    "es",
    "lib"
  ],
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/nodkz/graphql-compose-elasticsearch.git"
  },
  "keywords": [
    "graphql",
    "elastic",
    "elasticsearch",
    "graphql-compose"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nodkz/graphql-compose-elasticsearch/issues"
  },
  "homepage": "https://github.com/nodkz/graphql-compose-elasticsearch#readme",
  "dependencies": {
    "dox": "^0.9.0"
  },
  "peerDependencies": {
    "elasticsearch": "^12.1.3",
    "graphql": "^0.9.1",
    "graphql-compose": "^1.14.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.22.0",
    "babel-preset-env": "^1.2.2",
    "cz-conventional-changelog": "^2.0.0",
    "elasticsearch": "^12.1.3",
    "eslint": "^3.18.0",
    "eslint-config-airbnb-base": "^11.0.1",
    "eslint-config-prettier": "^1.0.2",
    "eslint-plugin-flowtype": "^2.30.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-prettier": "^2.0.0",
    "express": "^4.15.2",
    "express-graphql": "^0.6.3",
    "flow-bin": "^0.42.0",
    "graphql": "^0.9.1",
    "graphql-compose": "^1.17.3",
    "jest": "^19.0.2",
    "jest-babel": "^1.0.1",
    "npm-run-all": "^4.0.1",
    "prettier": "^0.22.0",
    "rimraf": "^2.5.4",
    "semantic-release": "^6.3.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ]
  },
  "scripts": {
    "build": "npm-run-all build:*",
    "build:lib": "rimraf lib && babel src --ignore __tests__,__mocks__ -d lib",
    "build:flow": "find ./src -name '*.js' -not -path '*/__*' | while read filepath; do cp $filepath `echo $filepath | sed 's/\\/src\\//\\/lib\\//g'`.flow; done",
    "dev": "nodemon -e js --ignore *test* --exec ./node_modules/.bin/babel-node ./examples/elastic50/index.js",
    "coverage": "jest --coverage",
    "lint": "eslint --ext .js ./src",
    "test": "jest",
    "test:watch": "jest --watch",
    "watch": "npm run test:watch",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  }
}
